@model TestVueWebApp.Repository.Models.Street
@using Newtonsoft.Json
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="app">
    <edit-component edit-street-url="@Url.Action("Edit", "Streets", new { StreetCd = Model.StreetCd } )" :street="@JsonConvert.SerializeObject(Model)"></edit-component>
</div>
@section Scripts{
    <script src="@Url.Content("~/bundle/streets.js")"></script>
}
