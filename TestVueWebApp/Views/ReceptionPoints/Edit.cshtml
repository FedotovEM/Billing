@model TestVueWebApp.Repository.Models.ReceptionPoint
@using Newtonsoft.Json
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="app">
    <reception-edit-component edit-reception-url="@Url.Action("Edit", "ReceptionPoints", new { ReceptionPointCd = Model.ReceptionPointCd } )" :reception="@JsonConvert.SerializeObject(Model)"></reception-edit-component>
</div>
@section Scripts{
    <script src="@Url.Content("~/bundle/ReceptionPoints.js")"></script>
}

