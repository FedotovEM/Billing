    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="app">
    <unit-edit-component edit-unit-url="@Url.Action("Edit", "Units", new { UnitCd = Model.UnitCd } )" :unit="@JsonConvert.SerializeObject(Model)"></unit-edit-component>
</div>
@section Scripts{
    <script src="@Url.Content("~/bundle/Units.js")"></script>
}
